.cs_search_icon {
    z-index: 99999;
    width: 70rpx;
    height: 70rpx;
    position: absolute;
    margin-top: -80rpx;
    text-align: center;
    line-height: 70rpx;
    margin-left: 20rpx;
    font-size: 30rpx;
}

.search_box {
    width: 100%;
    position: fixed;
    z-index: 2;
    margin-bottom: 30rpx;
    background-color: #666;
}

.search_top {
    color: #fff;
    height: 60rpx;
    overflow: hidden;
    padding: 10rpx 20rpx;
    position: relative;
}

.search_icon {
    width: 60rpx;
    height: 60rpx;
    margin-right: 40rpx;
}

.cs_search_btn {
    color: #666;
    position: absolute;
    top: 18rpx;
    margin-left: 10rpx;
}

.search_box input {
    background-color: #fff;
    color: #333;
    width: 350rpx;
    height: 60rpx;
    border-radius: 40rpx;
    overflow: hidden;
    padding-left: 50rpx;
    display: inline-block;
}

.cs_clear_btn {
    width: 60rpx;
    height: 60rpx;
    line-height: 60rpx;
    display: inline-block;
    padding-top: 10rpx;
    position: absolute;
}

.search_list {
    background-color: #fff;
    padding: 10px;
    position: fixed;
    z-index: 100;
    width: 100%;
    height: 900rpx;
    overflow: hidden;
}

.search_list .item {
    height: 60rpx;
    font-size: 30rpx;
    line-height: 60rpx;
}

.list_item {
    width: 337.5rpx;
    height: 482rpx;
    position: relative;
    border-radius: 25rpx;
    overflow: hidden;
    float: left;
    margin-left: 25rpx;
    margin-bottom: 25rpx;
}

.list_item image {
    position: relative;
    height: 100%;
    width: 100%;
}

.item_info {
    position: absolute;
    width: 317.5rpx;
    padding: 10rpx;
    height: 100rpx;
    bottom: 0;
    background-color: rgba(0,0,0,0.5);
    color: #fff;
    font-size: 25rpx;
}

.hot_item {
    height: 70rpx;
    margin: 20rpx;
    background-color: #666;
    color: #fff;
    line-height: 70rpx;
    font-size: 26rpx;
    padding: 0 10rpx;
    border-radius: 10rpx;
    display: inline-block;
}

.hot_title,.new_title {
    height: 80rpx;
    text-align: center;
    line-height: 80rpx;
    background-color: #666;
    font-size: 28rpx;
    color: #fff;
    margin-bottom: 20rpx;
}

.cs_swiper_item {
    position: relative;
}

.hot_video_title {
    position: absolute;
    color: #fff;
    bottom: 80rpx;
    width: 100%;
